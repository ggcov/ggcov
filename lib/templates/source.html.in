<table class="main">
  <thead>
    <tr>
      <th>Line</th>
      <th>Flow</th>
      <th>Count</th>
      <th>Block(s)</th>
      <th>Source</th>
    </tr>
  </thead>
  <tbody>
    {{#lines}}
    <tr>
      <td class="F{{status_short}} Clineno" width="1%"><a name="#L{{lineno}}"></a><a href="#L{{lineno}}">{{lineno}}</a></td>
      {{#flow_diagram}}
      <td class="Cflow" width="1%" rowspan="{{nlines}}"><img width="{{width}}" height="{{height}}" src="{{url}}"></td>
      {{/flow_diagram}}
      {{#flow_filler}}
      <td class="Cflow" width="1%"></td>
      {{/flow_filler}}
      <td class="F{{status_short}} Ccount" width="1%">{{count_if_instrumented}}</td>
      <td class="F{{status_short}} Cblocks" width="1%">{{blocks}}</td>
      <td class="F{{status_short}} Ctext{{#suppression_text}} tooltip{{/suppression_text}}">{{#suppression_text}}<span class="tooltiptext">suppressed: {{.}}</span>{{/suppression_text}}{{text}}</td>
    </tr>
    {{/lines}}
  </tbody>
</table>
